#define _XOPEN_SOURCE 777

#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
#include <sys/types.h>
#include <time.h>
#include <sys/socket.h>
#include <sys/un.h> 

void erreur(char * msg){
   perror(msg);
   exit(EXIT_FAILURE);
}

int main(int argc, char ** argv){
  if(argc!=2){
    fprintf(stderr,"Nombre d'arguments incorrect\n");
    fprintf(stderr,"Usage : %s <port>\n",argv[0]);
    exit(EXIT_FAILURE);
  }

  return EXIT_SUCCESS;
}
